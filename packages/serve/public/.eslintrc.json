{
  "root": true,
  "env": {
    "browser": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "extends": [
    "airbnb"
  ],
  "rules": {
    "max-len": 0,
    "no-plusplus": 0,
    "no-bitwise" : 0,
    "no-unused-expressions": 0,
    "react/destructuring-assignment": [0, "always"],
    "react/prop-types": 0,
    "react/no-deprecated": 0,
    "import/no-extraneous-dependencies": [2, { "devDependencies": true }],
    "import/no-dynamic-require": 0
  }
}
