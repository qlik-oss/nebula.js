{
  "name": "<%= name %>",
  "version": "0.1.0",
  "description": "<%= description %>",
  "license": "MIT",
  "author": {
    "name": "<%= user %>",
    "email": "<%= email %>"
  },
  "keywords": ["qlik", "nebula", "stardust"],
  "files": ["dist"],
  "engines": {
    "node": ">=18.10.0"
  },
  "main": "dist/<%= name %>.js",
  "module": "dist/<%= name %>.esm.js",
  "scripts": {
    "build": "nebula build",
