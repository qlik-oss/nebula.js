{
  "extends": ["airbnb-base", "plugin:mocha/recommended"],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "mocha/no-mocha-arrows": 0
  }
}
