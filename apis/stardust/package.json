{
  "name": "@nebula.js/stardust",
  "version": "5.0.0-alpha.1",
  "description": "Product and framework agnostic integration API for Qlik's Associative Engine",
  "license": "MIT",
  "author": "QlikTech International AB",
  "keywords": [
    "qlik",
    "qix",
    "sense",
    "nebula",
    "stardust"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qlik-oss/nebula.js.git",
    "directory": "apis/stardust"
  },
  "main": "index.js",
  "jsdelivr": "dist/stardust.js",
  "unpkg": "dist/stardust.js",
  "types": "types/index.d.ts",
  "files": [
    "LICENSE",
    "core",
    "dist",
    "types/index.d.ts"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup --config ../../rollup.config.js",
    "build:dev": "rollup --config ../../rollup.config.js",
    "build:watch": "rollup --config ../../rollup.config.js -w",
    "prepublishOnly": "rm -rf dist && yarn run build",
    "spec:stardust": "sy from-jsdoc -c ./api-spec/spec.conf.js",
    "spec:listbox": "sy from-jsdoc -c ./api-spec/listbox-spec.conf.js",
    "spec": "yarn run spec:stardust && yarn run spec:listbox",
    "ts": "sy to-dts -c ./api-spec/spec.conf.js",
    "version": "yarn run spec && yarn run ts && git add ./api-spec/spec.json ./api-spec/listbox-spec.json ./types/index.d.ts"
  },
  "dependencies": {
    "@types/qlik-engineapi": "12.67.16"
  },
  "devDependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.0",
    "@mui/material": "^5.15.0",
    "@nebula.js/conversion": "^5.0.0-alpha.1",
    "@nebula.js/enigma-mocker": "^5.0.0-alpha.1",
    "@nebula.js/locale": "^5.0.0-alpha.1",
    "@nebula.js/nucleus": "^5.0.0-alpha.1",
    "@nebula.js/supernova": "^5.0.0-alpha.1",
    "@nebula.js/theme": "^5.0.0-alpha.1",
    "@nebula.js/ui": "^5.0.0-alpha.1",
    "@scriptappy/cli": "0.10.0",
    "@scriptappy/from-jsdoc": "0.19.0",
    "@scriptappy/to-dts": "1.0.0-beta.4",
    "extend": "3.0.2",
    "node-event-emitter": "0.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-test-renderer": "18.2.0",
    "react-virtualized-auto-sizer": "1.0.20",
    "react-window": "1.8.10",
    "react-window-infinite-loader": "1.0.9",
    "regenerator-runtime": "0.14.1",
    "semver": "7.5.4"
  }
}
