        "types": {
          "optional": true,
          "kind": "array",
          "items": {
            "type": "#/definitions/TypeInfo"
          }
        },
        "themes": {
          "optional": true,
          "kind": "array",
          "items": {
            "type": "#/definitions/ThemeInfo"
          }
        },
        "anything": {
          "optional": true,
          "type": "object"
        }
      }
    },
    "Galaxy": {
      "kind": "interface",
      "entries": {
        "translator": {
          "type": "#/definitions/Translator"
