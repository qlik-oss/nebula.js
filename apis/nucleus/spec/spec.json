        },
        "unmount": {
          "kind": "function",
          "params": []
        }
      }
    },
    "BaseConfig": {
      "kind": "interface",
      "entries": {
        "element": {
          "optional": true,
          "type": "HTMLElement"
        },
        "optional": {
          "optional": true,
          "kind": "object",
          "entries": {
            "options": {
              "optional": true,
              "type": "object"
            },
            "properties": {
              "optional": true,
              "type": "object"
            }
          }
        }
      }
    },
    "Configuration": {
      "kind": "object",
      "entries": {
        "env": {
          "kind": "object",
          "entries": {}
        },
        "load": {
          "kind": "function",
          "params": []
        },
        "snapshot": {
          "kind": "object",
